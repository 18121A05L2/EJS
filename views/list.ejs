<%- include("header")  -%>
    <div class="box" id = "heading">
      <h1> <%= listTitle %> </h1>
    </div>
    <div class="box">
      <% for(let item of newItem){ %>
      <div class="item">
        <input type="checkbox">
        <p> <%= item %> </p>
      </div>
      <% } %>

      <form class="item" action="/" method="post">
        <input type="text" name="input1" placeholder="new item" autocomplete="off">
        <button type="submit" name="list" value= <%= listTitle %> >+</button>
      </form>
    </div>
<%- include("footer")  -%>
